{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "myexpenses-datafactory"
		},
		"LS_AzureSQL_Expense_Sink_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureSQL_Expense_Sink'"
		},
		"LS_Historical_Blob_Source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Historical_Blob_Source'"
		},
		"LS_AzureSQL_Expense_Sink_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "myexpenses-db-server.database.windows.net"
		},
		"LS_AzureSQL_Expense_Sink_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "myexpenses_db"
		},
		"LS_AzureSQL_Expense_Sink_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "CloudSAac3e3eda"
		},
		"bulkupload_trigger_properties_PL_BulkUpload_Excel_parameters_ProcessingTimeStamp": {
			"type": "string",
			"defaultValue": "@trigger().startTime"
		},
		"bulkupload_trigger_properties_PL_BulkUpload_Excel_parameters_Filename": {
			"type": "string",
			"defaultValue": "@{last(split(trigger().outputs.body.subject,'/'))}"
		},
		"bulkupload_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/dc047e3c-dc08-457f-92cc-3a531d30bb7b/resourceGroups/myexpenses-suite-rg/providers/Microsoft.Storage/storageAccounts/myexpensesstorageacc"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "myexpenses-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureSQL_Expense_Sink_password": {
						"value": "[parameters('LS_AzureSQL_Expense_Sink_password')]"
					},
					"LS_Historical_Blob_Source_connectionString": {
						"value": "[parameters('LS_Historical_Blob_Source_connectionString')]"
					},
					"LS_AzureSQL_Expense_Sink_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSQL_Expense_Sink_properties_typeProperties_server')]"
					},
					"LS_AzureSQL_Expense_Sink_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSQL_Expense_Sink_properties_typeProperties_database')]"
					},
					"LS_AzureSQL_Expense_Sink_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSQL_Expense_Sink_properties_typeProperties_userName')]"
					},
					"bulkupload_trigger_properties_PL_BulkUpload_Excel_parameters_ProcessingTimeStamp": {
						"value": "[parameters('bulkupload_trigger_properties_PL_BulkUpload_Excel_parameters_ProcessingTimeStamp')]"
					},
					"bulkupload_trigger_properties_PL_BulkUpload_Excel_parameters_Filename": {
						"value": "[parameters('bulkupload_trigger_properties_PL_BulkUpload_Excel_parameters_Filename')]"
					},
					"bulkupload_trigger_properties_typeProperties_scope": {
						"value": "[parameters('bulkupload_trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}