{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Historical_Source_CSV",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SinkDB_BulkUpload",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1",
					"description": "Format TransactionDate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CardName as string,",
				"          AccountType as string,",
				"          TransactionDate as string,",
				"          Description as string,",
				"          Amount as decimal(10,0),",
				"          ExpenseType as string,",
				"          SubCategory as string,",
				"          Category as string,",
				"          AccountNo_Ref as string,",
				"          StatementDate as date,",
				"          SourceFileName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(TransactionDate = ERROR_FUNCTION('')) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          Id as integer,",
				"          TransactionDate as date,",
				"          Description as string,",
				"          Amount as decimal(18,2),",
				"          ExpenseType as string,",
				"          CardName as string,",
				"          AccountType as string,",
				"          Category as string,",
				"          SubCategory as string,",
				"          AccountNo_Ref as string,",
				"          SourceFileName as string,",
				"          ProcessingTimestamp as timestamp,",
				"          StatementDate as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          TransactionDate,",
				"          Description,",
				"          ExpenseType,",
				"          CardName,",
				"          AccountType,",
				"          Category,",
				"          SubCategory,",
				"          AccountNo_Ref,",
				"          SourceFileName",
				"     )) ~> sink1"
			]
		}
	}
}