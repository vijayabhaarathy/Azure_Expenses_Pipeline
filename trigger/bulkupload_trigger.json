{
	"name": "bulkupload_trigger",
	"properties": {
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "PL_BulkUpload_Excel",
					"type": "PipelineReference"
				},
				"parameters": {
					"ProcessingTimeStamp": "@trigger().startTime",
					"Filename": "@{last(split(trigger().outputs.body.subject,'/'))}"
				}
			}
		],
		"type": "BlobEventsTrigger",
		"typeProperties": {
			"blobPathBeginsWith": "/myexpenses-bulkupload/blobs/",
			"ignoreEmptyBlobs": true,
			"scope": "/subscriptions/dc047e3c-dc08-457f-92cc-3a531d30bb7b/resourceGroups/myexpenses-suite-rg/providers/Microsoft.Storage/storageAccounts/myexpensesstorageacc",
			"events": [
				"Microsoft.Storage.BlobCreated"
			]
		}
	}
}